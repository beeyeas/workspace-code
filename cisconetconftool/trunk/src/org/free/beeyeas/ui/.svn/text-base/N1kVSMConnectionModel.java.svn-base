package org.free.beeyeas.ui;

public class N1kVSMConnectionModel {
	String vsmUser;
	String vsmIpAddress;
	String vsmpassword;
	@SuppressWarnings("unused")
	private N1kVSMConnectionModel() {
	}
	public N1kVSMConnectionModel(String user,String password, String ipAddress){
		this.vsmUser = user;
		this.vsmIpAddress = ipAddress;
		this.vsmpassword = password;
	}
	public String getVsmUser() {
		return vsmUser;
	}
	public void setVsmUser(String vsmUser) {
		this.vsmUser = vsmUser;
	}
	public String getVsmIpAddress() {
		return vsmIpAddress;
	}
	public void setVsmIpAddress(String vsmIpAddress) {
		this.vsmIpAddress = vsmIpAddress;
	}
	public String getVsmpassword() {
		return vsmpassword;
	}
	public void setVsmpassword(String vsmpassword) {
		this.vsmpassword = vsmpassword;
	}
}
